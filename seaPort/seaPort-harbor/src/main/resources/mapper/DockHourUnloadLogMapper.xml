<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iwip.harbor.mapper.DockHourUnloadLogMapper">

    <resultMap type="com.iwip.harbor.domain.DockHourUnloadLog" id="DockHourUnloadLogResult">
        <result property="dhuId"    column="dhu_id"    />
        <result property="planId"    column="plan_id"    />
        <result property="startTime"    column="start_time"    />
        <result property="endTime"    column="end_time"    />
        <result property="unloadWeight"    column="unload_weight"    />
        <result property="unloadNum"    column="unload_num"    />
        <result property="exceptionStatus"    column="exception_status"    />
        <result property="exceptionReason"    column="exception_reason"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="userId"    column="user_id"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="deptId"    column="dept_id"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
        <result property="remark01"    column="remark01"    />
        <result property="remark02"    column="remark02"    />
        <result property="remark03"    column="remark03"    />
    </resultMap>

    <sql id="selectDockHourUnloadLogVo">
        select exception_status,exception_reason,dhu_id, plan_id, start_time, end_time, unload_weight, unload_num, del_flag, user_id, create_by, create_time, dept_id, update_by, update_time, remark, remark01, remark02, remark03 from dock_hour_unload_log
    </sql>

    <select id="selectDockHourUnloadLogList" parameterType="com.iwip.harbor.domain.DockHourUnloadLog" resultMap="DockHourUnloadLogResult">
        <include refid="selectDockHourUnloadLogVo"/>
        <where>
            AND del_flag = '0'
            <if test="planId != null "> and plan_id = #{planId}</if>
            <if test="startTime != null "> and start_time = #{startTime}</if>
            <if test="exceptionReason != null "> and exception_reason = #{exceptionReason}</if>
            <if test="exceptionStatus != null "> and exception_status = #{exceptionStatus}</if>
            <if test="startTime != null "> and start_time = #{startTime}</if>
            <if test="endTime != null "> and end_time = #{endTime}</if>
            <if test="unloadWeight != null "> and unload_weight = #{unloadWeight}</if>
            <if test="unloadNum != null "> and unload_num = #{unloadNum}</if>
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="deptId != null "> and dept_id = #{deptId}</if>
            <if test="remark01 != null  and remark01 != ''"> and remark01 = #{remark01}</if>
            <if test="remark02 != null  and remark02 != ''"> and remark02 = #{remark02}</if>
            <if test="remark03 != null  and remark03 != ''"> and remark03 = #{remark03}</if>
        </where>
        order by dhu_id
    </select>

    <select id="selectDockHourUnloadLogByDhuId" parameterType="Long" resultMap="DockHourUnloadLogResult">
        <include refid="selectDockHourUnloadLogVo"/>
        where dhu_id = #{dhuId}
        AND del_flag = '0'
    </select>

    <select id="selectDockHourUnloadLogByPlanId" parameterType="Long" resultMap="DockHourUnloadLogResult">
        <include refid="selectDockHourUnloadLogVo"/>
        where plan_id = #{planId}
        AND del_flag = '0'
    </select>

    <select id="screenHourUnloadLogList" parameterType="Long" resultType="com.iwip.harbor.domain.vo.DockPlanEfficiencyHourVo">
        SELECT DATE_FORMAT(start_time, '%H') time,
               unload_weight unloadWeight,
               unload_num unloadNum,
                exception_reason exceptionReason,
                exception_status exceptionStatus
        FROM (SELECT *, ROW_NUMBER() OVER ( PARTITION BY plan_id ORDER BY dhu_id  DESC ) AS rn
              FROM dock_hour_unload_log
              WHERE del_flag = '0') AS ranked
        WHERE
            rn &lt;= 12
          AND plan_id = #{planId}

        ORDER BY dhu_id
    </select>


    <insert id="insertDockHourUnloadLog" parameterType="com.iwip.harbor.domain.DockHourUnloadLog" useGeneratedKeys="true" keyProperty="dhuId">
        insert into dock_hour_unload_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="dhuId != null">dhu_id,</if>
            <if test="planId != null">plan_id,</if>
            <if test="startTime != null">start_time,</if>
            <if test="endTime != null">end_time,</if>
            <if test="unloadWeight != null">unload_weight,</if>
            <if test="unloadNum != null">unload_num,</if>
            <if test="exceptionStatus != null">exception_status,</if>
            <if test="exceptionReason != null">exception_reason,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="userId != null">user_id,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="deptId != null">dept_id,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
            <if test="remark01 != null">remark01,</if>
            <if test="remark02 != null">remark02,</if>
            <if test="remark03 != null">remark03,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="dhuId != null">#{dhuId},</if>
            <if test="planId != null">#{planId},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="endTime != null">#{endTime},</if>
            <if test="unloadWeight != null">#{unloadWeight},</if>
            <if test="unloadNum != null">#{unloadNum},</if>
            <if test="exceptionStatus != null">#{exceptionStatus},</if>
            <if test="exceptionReason != null">#{exceptionReason},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="userId != null">#{userId},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="deptId != null">#{deptId},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
            <if test="remark01 != null">#{remark01},</if>
            <if test="remark02 != null">#{remark02},</if>
            <if test="remark03 != null">#{remark03},</if>
         </trim>
    </insert>

    <update id="updateDockHourUnloadLog" parameterType="com.iwip.harbor.domain.DockHourUnloadLog">
        update dock_hour_unload_log
        <trim prefix="SET" suffixOverrides=",">
            <if test="planId != null">plan_id = #{planId},</if>
            <if test="startTime != null">start_time = #{startTime},</if>
            <if test="endTime != null">end_time = #{endTime},</if>
            <if test="unloadWeight != null">unload_weight = #{unloadWeight},</if>
            <if test="unloadNum != null">unload_num = #{unloadNum},</if>
            <if test="exceptionStatus != null">exception_status = #{exceptionStatus},</if>
            exception_reason = #{exceptionReason},
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="userId != null">user_id = #{userId},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="deptId != null">dept_id = #{deptId},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="remark01 != null">remark01 = #{remark01},</if>
            <if test="remark02 != null">remark02 = #{remark02},</if>
            <if test="remark03 != null">remark03 = #{remark03},</if>
        </trim>
        where dhu_id = #{dhuId}
    </update>

    <delete id="deleteDockHourUnloadLogByDhuId" parameterType="Long">
        update dock_hour_unload_log set del_flag = '1' where dhu_id = #{dhuId}
    </delete>

    <delete id="deleteDockHourUnloadLogByDhuIds" parameterType="String">
        update dock_hour_unload_log set del_flag = '1' where dhu_id in
        <foreach item="dhuId" collection="array" open="(" separator="," close=")">
            #{dhuId}
        </foreach>
    </delete>
</mapper>
