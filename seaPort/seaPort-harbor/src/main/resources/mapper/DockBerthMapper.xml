    <sql id="selectDockBerthVo">
        select berth_id,berth_code, berth_hp_id, berth_name,
               berth_status, berth_sort, dept_id, berth_geo_json,
               del_flag, user_id, create_by, create_time, update_by,
               update_time, remark, remark01, remark02, remark03 from dock_berth
    </sql>



    <select id="selectDockBerthList" parameterType="com.iwip.harbor.domain.DockBerth" resultMap="DockBerthResult">
<!--        <include refid="selectDockBerthVo"/>-->
        select dp.berth_id, dp.berth_code, dp.berth_hp_id,
               dp.berth_name, dp.berth_status, dp.berth_sort,
               dp.dept_id, dp.berth_geo_json, dp.del_flag,
               dp.user_id, dp.create_by, dp.create_time, dp.update_by,
               dp.update_time, dp.remark, dp.remark01, dp.remark02,
               dp.remark03 from dock_berth as dp
            left join sys_dept as d on dp.dept_id = d.dept_id
        <where>
            AND dp.del_flag = '0'
            <if test="berthHpId != null  and berthHpId != ''"> and dp.berth_hp_id = #{dbHpId}</if>
            <if test="berthName != null  and berthName != ''"> and dp.berth_name like concat('%', #{berthName}, '%')</if>
            <if test="berthStatus != null  and berthStatus != ''"> and dp.berth_status = #{berthStatus}</if>
            <if test="berthCode != null  and berthCode != ''"> and dp.berth_code = #{berthCode}</if>
            <if test="berthSort != null "> and dp.berth_sort = #{berthSort}</if>
            <if test="deptId != null  and deptId != ''"> and dp.dept_id = #{deptId}</if>
            <if test="berthGeoJson != null  and geoJson != ''"> and dp.berth_geo_json = #{berthGeoJson}</if>
            <if test="userId != null "> and dp.user_id = #{userId}</if>
            <if test="createBy != null  and createBy != ''"> and dp.create_by = #{createBy}</if>
            <if test="createTime != null "> and dp.create_time = #{createTime}</if>
            <if test="updateBy != null  and updateBy != ''"> and dp.update_by = #{updateBy}</if>
            <if test="updateTime != null "> and dp.update_time = #{updateTime}</if>
            <if test="remark01 != null  and remark01 != ''"> and dp.remark01 = #{remark01}</if>
            <if test="remark02 != null  and remark02 != ''"> and dp.remark02 = #{remark02}</if>
            <if test="remark03 != null  and remark03 != ''"> and dp.remark03 = #{remark03}</if>
            ${params.dataScope}
        </where>
        ORDER BY dp.berth_hp_id, dp.create_time
    </select>

    <select id="screenBerthStatusList" parameterType="com.iwip.harbor.domain.DockBerth" resultType="com.iwip.harbor.domain.screen.ScreenPlanStatusVo">
        SELECT
            '0' AS STATUS,
            COUNT( b.berth_id ) AS count
        FROM
            dock_berth b
        WHERE
            b.berth_status = '0'
          AND b.del_flag = '0' UNION ALL
        SELECT
            '1',
            COUNT( b.berth_id )
        FROM
            dock_berth b
        WHERE
            b.berth_status = '1'
          AND b.del_flag = '0' UNION ALL
        SELECT
            '2',
            COUNT( b.berth_id )
        FROM
            dock_berth b
        WHERE
            b.berth_status = '2'
          AND b.del_flag = '0' UNION ALL
        SELECT
            '3',
            COUNT( b.berth_id )
        FROM
            dock_berth b
        WHERE
            b.berth_status = '3'
          AND b.del_flag = '0'
    </select>

    <select id="selectDockBerthByDbId" parameterType="Long" resultMap="DockBerthResult">
        <include refid="selectDockBerthVo"/>
        where berth_id = #{berthId}
    </select>

    <select id="selectDockBerthByName" parameterType="String" resultMap="DockBerthResult">
        <include refid="selectDockBerthVo"/>
        where del_flag = '0'  and  berth_name = #{berthName}
    </select>

    <select id="selectDockBerthByCode" parameterType="String" resultMap="DockBerthResult">
        <include refid="selectDockBerthVo"/>
        where del_flag = '0'  and  berth_code = #{berthCode}
    </select>

    <select id="selectDockBerthByNameOrCode" parameterType="String" resultMap="DockBerthResult">
        <include refid="selectDockBerthVo"/>
        where del_flag = '0'  and
        (
        berth_name = #{berthName}
        or berth_code = #{berthName}
        )
    </select>

    <select id="selectDockBerthByBerthHpIdId" parameterType="Long" resultMap="DockBerthResult">
        <include refid="selectDockBerthVo"/>
        where del_flag = '0' and berth_hp_id = #{berthHpId}
    </select>

    <select id="selectDockBerthByStatus" resultMap="DockBerthResult">
        <include refid="selectDockBerthVo"/>
        where del_flag = '0' and berth_status in
        <foreach collection="berthStatusList" item="berthStatus" open="(" separator="," close=")">
            #{berthStatus}
        </foreach>
    </select>

