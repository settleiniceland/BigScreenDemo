    </resultMap>

    <sql id="selectDockUnloadDetailVo">
        select dud_id,record_status, du_id, start_time, end_time,pause_interval, reason, dept_id, del_flag, user_id, create_by, create_time, update_by, update_time,remark, remark01, remark02, remark03 from dock_unload_detail
    </sql>

    <select id="selectDockUnloadDetailList" parameterType="com.iwip.harbor.domain.DockUnloadDetail" resultMap="DockUnloadDetailResult">
        <include refid="selectDockUnloadDetailVo"/>
        <where>
            AND del_flag = '0'
            <if test="duId != null "> and du_id = #{duId}</if>
            <if test="startTime != null "> and start_time = #{startTime}</if>
            <if test="endTime != null "> and end_time = #{endTime}</if>
            <if test="pauseInterval != null  and pauseInterval != ''"> and pause_interval = #{pause_interval}</if>
            <if test="reason != null  and reason != ''"> and reason = #{reason}</if>
            <if test="deptId != null  and deptId != ''"> and dept_id = #{deptId}</if>
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="createBy != null  and createBy != ''"> and create_by = #{createBy}</if>
            <if test="createTime != null "> and create_time = #{createTime}</if>
            <if test="updateBy != null  and updateBy != ''"> and update_by = #{updateBy}</if>
            <if test="updateTime != null "> and update_time = #{updateTime}</if>
            <if test="remark01 != null  and remark01 != ''"> and remark01 = #{remark01}</if>
            <if test="remark02 != null  and remark02 != ''"> and remark02 = #{remark02}</if>
            <if test="remark03 != null  and remark03 != ''"> and remark03 = #{remark03}</if>
        </where>
    </select>

    <select id="selectDockUnloadDetailByDudId" parameterType="Long" resultMap="DockUnloadDetailResult">
        <include refid="selectDockUnloadDetailVo"/>
        where dud_id = #{dudId}
    </select>

    <select id="selectUnloadDetailByRecordByDuId" parameterType="Long" resultMap="DockUnloadDetailResult">
        <include refid="selectDockUnloadDetailVo"/>
        where du_id = #{duId} and record_status = '1'
    </select>


    <select id="selectDockUnloadDetailByDuId" parameterType="Long" resultMap="DockUnloadDetailResult">
        <include refid="selectDockUnloadDetailVo"/>
        where du_id = #{duId}
        order by du_id
    </select>

    <select id="selectDetailDescByDuId" parameterType="Long" resultMap="DockUnloadDetailResult">
        <include refid="selectDockUnloadDetailVo"/>
        where du_id = #{duId} ORDER BY create_time DESC limit 1;
    </select>

    <select id="selectDetailDescList" parameterType="Long" resultMap="DockUnloadDetailResult">
        <include refid="selectDockUnloadDetailVo"/>
        where du_id = #{duId} ORDER BY create_time DESC;
    </select>
    <select id="selectByDuIds" resultMap="DockUnloadDetailResult" parameterType="java.util.List">
        <include refid="selectDockUnloadDetailVo"/>
        WHERE del_flag = '0' AND du_id IN (
            <foreach collection="list" item="item" separator=",">
                #{item}
            </foreach>
        )
    </select>

    <insert id="insertDockUnloadDetail" parameterType="com.iwip.harbor.domain.DockUnloadDetail" useGeneratedKeys="true" keyProperty="dudId">
