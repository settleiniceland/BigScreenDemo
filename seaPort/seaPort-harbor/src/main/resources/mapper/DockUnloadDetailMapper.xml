    <delete id="deleteDockUnloadDetailByDudId" parameterType="Long">
        delete from dock_unload_detail where dud_id = #{dudId}
    </delete>

    <delete id="deleteDockUnloadDetailByDudIds" parameterType="String">
        delete from dock_unload_detail where dud_id in
        <foreach item="dudId" collection="array" open="(" separator="," close=")">
            #{dudId}
        </foreach>
    </delete>
