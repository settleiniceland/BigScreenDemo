<mapper namespace="com.iwip.harbor.mapper.DockUnloadDetailMapper">

    <resultMap type="com.iwip.harbor.domain.DockUnloadDetail" id="DockUnloadDetailResult">
        <result property="dudId"    column="dud_id"    />
        <result property="duId"    column="du_id"    />
        <result property="startTime"    column="start_time"    />
        <result property="endTime"    column="end_time"    />
        <result property="pauseInterval"    column="pause_interval"    />
