<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iwip.harbor.mapper.DockLoadRateMapper">
    
    <resultMap type="com.iwip.harbor.domain.DockLoadRate" id="DockLoadRateResult">
        <result property="id"    column="id"    />
        <result property="planId"    column="plan_id"    />
        <result property="berthName"    column="berth_name"    />
        <result property="rate"    column="rate"    />
        <result property="createTime"    column="create_time"    />
        <result property="endTime"    column="end_time"    />
    </resultMap>

    <sql id="selectDockLoadRateVo">
        select id, plan_id,berth_name, rate,create_time,end_time from dock_load_rate
    </sql>

    <select id="selectDockLoadRateList" parameterType="com.iwip.harbor.domain.DockLoadRate" resultMap="DockLoadRateResult">
        <include refid="selectDockLoadRateVo"/>
        <where>  
            <if test="planId != null "> and planId = #{planId}</if>
            <if test="rate != null "> and rate = #{rate}</if>
        </where>
    </select>

    <select id="selectRateListBetweenTime"  resultMap="DockLoadRateResult">
        <include refid="selectDockLoadRateVo"/>
        <where>
            AND create_time &gt;= #{startTime}
            AND end_time &lt; #{stopTime}
            AND NOT (
            create_time &gt;= #{notStartTime} 
            AND end_time &lt; #{notEndTime})
        </where>
    </select>


    <select id="selectDockLoadRateById" parameterType="Long" resultMap="DockLoadRateResult">
        <include refid="selectDockLoadRateVo"/>
        where id = #{id}
    </select>

    <select id="selectDockLoadRateListByIdAndName"  resultMap="DockLoadRateResult">
        <include refid="selectDockLoadRateVo"/>
        where plan_id = #{planId} and berth_name = #{berthName}
    </select>


    <insert id="insertDockLoadRate" parameterType="com.iwip.harbor.domain.DockLoadRate" useGeneratedKeys="true" keyProperty="id">
        insert into dock_load_rate
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="planId != null">plan_id,</if>
            <if test="berthName != null">berth_name,</if>
            <if test="rate != null">rate,</if>
            <if test="createTime != null">create_time,</if>
            <if test="endTime != null">end_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="planId != null">#{planId},</if>
            <if test="berthName != null">#{berthName},</if>
            <if test="rate != null">#{rate},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="endTime != null">#{endTime},</if>
         </trim>
    </insert>

    <update id="updateDockLoadRate" parameterType="com.iwip.harbor.domain.DockLoadRate">
        update dock_load_rate
        <trim prefix="SET" suffixOverrides=",">
            <if test="planId != null">plan_id = #{planId},</if>
            <if test="berthName != null">berth_name = #{berthName},</if>
            <if test="rate != null">rate = #{rate},</if>
            <if test="createTime != null">create_time = #{createTime}</if>
            <if test="endTime != null">end_time = #{endTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteDockLoadRateById" parameterType="Long">
        delete from dock_load_rate where id = #{id}
    </delete>

    <delete id="deleteDockLoadRateByIds" parameterType="String">
        delete from dock_load_rate where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>